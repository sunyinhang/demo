<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.haiercash.payplatform.common.dao.ChannelConfigurationDao">

    <select id="selectConfig" resultType="com.haiercash.payplatform.common.data.ChannelConfiguration">
        select * from CHANNEL_CONFIGURATION where CHANNEL_NO = #{channelNo} AND ACTIVEFLAG = #{activeFlag}
    </select>

</mapper>
