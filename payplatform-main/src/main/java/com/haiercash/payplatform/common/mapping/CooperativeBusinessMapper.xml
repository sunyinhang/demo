<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.haiercash.payplatform.common.dao.CooperativeBusinessDao">

    <select id="selectBycooperationcoed" resultType="com.haiercash.payplatform.common.data.CooperativeBusiness" >
        select * from COOPERATIVEBUSINESS t  where t.cooperationcoed = #{cooperationcoed, jdbcType=VARCHAR}
    </select>

</mapper>